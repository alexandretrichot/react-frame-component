{
  "extends": ["airbnb", "prettier"],
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true,
      "jsx": true
    }
  },
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "rules": {
    "react/forbid-prop-types": "off",
    "react/prop-types": [0, { "ignore": ["children"]}],
    "no-underscore-dangle": "off"
  }
}
